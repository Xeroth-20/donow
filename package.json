		"nodemon": "^2.0.7",
		"postcss-loader": "^6.1.0",
		"sass-loader": "^12.1.0",
		"style-loader": "^2.0.0",
		"ts-loader": "^9.2.3",
		"typescript": "^4.3.2",
		"webpack": "^5.39.0",
		"webpack-cli": "^4.7.2",
		"webpack-dev-server": "^3.11.2",
		"webpack-merge": "^5.8.0",
		"webpack-node-externals": "^3.0.0"
	}
